{
    "manifest_version": 2,
    "name": "MyNixOS Search Engine",
    "description": "Adds MyNixOS search engine",
    "version": "1.0",
    "author": "Kiara Grouwstra",
    "homepage_url": "https://github.com/KiaraGrouwstra/firefox-mynixos-extension",
    "developer": {
      "name": "Kiara Grouwstra",
      "url": "https://github.com/KiaraGrouwstra"
    },
    "chrome_settings_overrides": {
      "search_provider": {
        "name": "MyNixOS",
        "search_url":"https://mynixos.com/search?q={searchTerms}",
        "keyword": "@mynixos",
        "favicon_url": "https://nixos.org/favicon.ico",
        "is_default": false,
        "encoding": "UTF-8"
      }
    }
}
